<%# @messages.each do |message| %>
  <div class="message">
    <div class="top-message">
      <div class="sender">
        <%= message.user.name %>
      </div>
      <div class="time-stamp">
        <%= l message.created_at %>
      </div>
    </div>
    <div class="bottom-message">
      <div class="text-photo">
        <%= message.content %>
      </div>
    </div>
  </div>
<%# end %>